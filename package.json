{
  "name": "react-native-swipe-predictor",
  "version": "1.0.0",
  "description": "Physics-based gesture predictor for React Native apps",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "rn-swipe-predictor.podspec"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build",
    "build:rust:ios": "cd src/rust && cargo lipo --release",
    "build:rust:android": "cd src/rust && ./build-android.sh",
    "build:rust": "npm run build:rust:ios && npm run build:rust:android",
    "example": "cd example && npm start",
    "test": "jest"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "gesture",
    "swipe",
    "prediction",
    "physics"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/react-native-swipe-predictor.git"
  },
  "author": "Your Name <email@example.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yourusername/react-native-swipe-predictor/issues"
  },
  "homepage": "https://github.com/yourusername/react-native-swipe-predictor#readme",
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": ">=2.0.0",
    "react-native-reanimated": ">=3.0.0"
  },
  "devDependencies": {
    "@react-native-community/bob": "^0.18.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.72.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "react": "^18.0.0",
    "react-native": "^0.72.0",
    "typescript": "^5.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  }
}